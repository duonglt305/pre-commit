{
    "name": "duolee/pre-commit",
    "version": "1.0",
    "description": "The pre-commit system for the Laravel Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Duong Le",
            "email": "duonglt30597@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "DuoLee\\PreCommit\\": "./src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DuoLee\\PreCommit\\PreCommitServiceProvider"
            ]
        }
    }
}
